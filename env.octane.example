APP_NAME="Laravel Octane"
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_TIMEZONE=UTC
APP_URL=http://localhost:8080

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Database Configuration
DB_CONNECTION=mysql
DB_HOST=mysql
DB_PORT=3306
DB_DATABASE=laravel
DB_USERNAME=laravel
DB_PASSWORD=secret

# Session Configuration (Redis for Octane)
SESSION_DRIVER=redis
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

# Broadcasting
BROADCAST_CONNECTION=redis

# Filesystem
FILESYSTEM_DISK=local

# Queue Configuration (Redis for better performance)
QUEUE_CONNECTION=redis

# Cache Configuration (Redis for Octane)
CACHE_STORE=redis
CACHE_PREFIX=laravel_octane

# Redis Configuration
REDIS_CLIENT=phpredis
REDIS_HOST=redis
REDIS_PASSWORD=null
REDIS_PORT=6379
REDIS_DB=0

# Cache database numbers for different purposes
REDIS_CACHE_DB=1
REDIS_SESSION_DB=2
REDIS_QUEUE_DB=3

# Mail Configuration
MAIL_MAILER=log
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_ENCRYPTION=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# AWS Services (if needed)
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

# FrankenPHP Configuration (internal port behind nginx)
SERVER_NAME=:8000
FRANKENPHP_CONFIG="worker /app/public/frankenphp-worker.php"
FRANKENPHP_NUM_WORKERS=auto
FRANKENPHP_NUM_THREADS=2

# Octane Configuration (internal port behind nginx)
OCTANE_SERVER=frankenphp
OCTANE_HTTPS=false
OCTANE_HOST=0.0.0.0
OCTANE_PORT=8000
OCTANE_WORKERS=auto
OCTANE_TASK_WORKERS=auto
OCTANE_MAX_REQUESTS=500
OCTANE_WATCH=true

# Telescope (if using)
TELESCOPE_ENABLED=true

# Horizon (if using)
HORIZON_PREFIX=horizon:

# Performance Monitoring
LOG_LEVEL=info
QUERY_DETECTOR_ENABLED=false

# Development Tools
VITE_APP_NAME="${APP_NAME}" 